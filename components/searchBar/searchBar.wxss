/* components/searchBar.wxss */
.searchBar-group{
  padding: 20rpx;
  background-color: #00b26a;
}

.searchBar{
  height: 60rpx;
  width: 100%;
  border-radius: 20rpx;
  background-color: white;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 8%;
}

.search{
  height: 60rpx;
  width: 100%;
  padding: 10rpx 20rpx;
  border-radius: 20rpx;
  background-color: white;
  box-sizing: border-box;
}

.search-input{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 12px;
}